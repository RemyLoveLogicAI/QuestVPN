# Staging environment configuration
ENVIRONMENT=staging
REGION=west

# Monitoring
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=staging_password

# Alerting  
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/STAGING/URL
ALERT_EMAIL=staging@questvpn.com

# Backup
BACKUP_DIR=/opt/questvpn/backups
BACKUP_RETENTION_DAYS=7
BACKUP_S3_BUCKET=questvpn-backups-staging

# Database
DB_RETENTION_DAYS=30
LOG_RETENTION_DAYS=7

# Performance
MAX_PEERS_PER_SERVER=50
RATE_LIMIT_PER_IP=1000
